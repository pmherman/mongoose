    <header>
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="navbar-brand">NYTimes Scraper</div>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="/">Home</a></li>
                    <li><a href="/saved">Saved</a></li>
                    <li>
                         <button type="button" id="scrape" class="btn btn-success" data-toggle="modal" data-target="#scrapeModal">SCRAPE HEADLINES</button>
                    </li>
                </ul>
                </div><!-- /.navbar-collapse -->
                <div class="jumbotron">
                    <h1 class="text-center">NYTimes Scraper</h1>
                </div>
            </div><!-- /.container-fluid -->
        </nav>
    </header>
    <div class="container-fluid">
        <div class="col-lg-12">
            <h1>New Articles</h1>
                {{#each headline}}
                    {{#unless this.saved}}
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h4>{{this.headline}}</h4>
                            </div>
                            <div class="panel-body">
                                <p>{{this.summary}}</p>
                                <a href={{this.url}} target="_blank">{{this.url}}</a>
                                <br>
                            </div>
                            <div class="panel-footer">
                                <form action="/save/{{this.id}}" method="POST">
                                    <button type="submit" class="btn btn-warning save" value="{{this.status}}">{{this.status}}</button>
                                </form>
                            </div>                        
                        </div>
                    {{/unless}}    
                {{/each}}
        </div>
    </div>

{{!-- Scrape Button Modal --}}
